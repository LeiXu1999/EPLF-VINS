<launch>

    <arg name="config_path" default = "$(find feature_tracker)/../config/euroc/euroc_config_fix_extrinsic.yaml" />
	  <arg name="vins_path" default = "$(find feature_tracker)/../config/../" />

    <node pkg="rosbag" type="play" name="playe"  args="/home/dragon/ros_ws/small.bag"/> 
    <node name="lf_linefeature_tracker" pkg="feature_tracker" type="lf_LineFeature_tracker" output="screen">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>    

</launch>

